        xmlns:local="clr-namespace:CoolWork"
        xmlns:Controls="clr-namespace:CoolWork.Controls;assembly=CoolWork.Controls" x:Class="CoolWork.EmployeeEditor"
        mc:Ignorable="d"
        Title="EmployeeEditor" Height="384" Width="429">
