        <Controls:EmployeeControl x:Name="employeeControl" Margin="10,10,0,25"/>
        <Button x:Name="btnCancel" Content="Отмена" HorizontalAlignment="Left" Margin="340,334,0,0" VerticalAlignment="Top" RenderTransformOrigin="-6.94,-1.49" Width="69" Click="btnCancel_Click"/>
        <Button x:Name="btnOk" Content="OK" HorizontalAlignment="Left" Margin="250,334,0,0" VerticalAlignment="Top" RenderTransformOrigin="-6.94,-1.49" Width="75" Click="btnOk_Click"/>

    </Grid>
</Window>
